{
  "short_name": "chromeextension",
  "name": "Youtube Extension",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "storage", "unlimitedStorage", "background"],
  "icons": {
    "16": "assets/logo-16.png",
    "48": "assets/logo-16.png",
    "128": "assets/logo-16.png"
  },

  "version": "1.0"
}
